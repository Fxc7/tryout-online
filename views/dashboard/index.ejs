<main x-data="{ indexDelete: null }">
  <div class="p-4 mx-auto max-w-screen-2xl md:p-6">
    <div class="grid grid-cols-12 gap-4 md:gap-6">
      <% if (page === 'dashboard') { %>
        <div class="col-span-12 space-y-6">
          <%- include('./grids.ejs') %>
        </div>
      <% } %>
      <% if (page === 'users') { %>
        <div class="col-span-12">
          <%- include('./listUsers.ejs') %>
        </div>
        <% } %>
        <% if (/sd|smp|smk/.test(page)) { %>
          <div class="col-span-12">
            <%- include('./tableSoal.ejs') %>
          </div>
      <% } %>
    </div>
  </div>
</main>